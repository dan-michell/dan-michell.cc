---
import LinkIcon from '../common/LinkIcon.astro'

interface Props {
	
}


const link_props = [
	{
		href: "/",
		displayName: "Home"
	},
	{
		href: "/blog",
		displayName: "Blog"
	},
	{
		href: "/reading",
		displayName: "Reading"
	},
	{
		href: "/about",
		displayName: "About"
	},
]
---
<div class="flex flex-col gap-2">
	{link_props.map(iconProps => {
	return <LinkIcon {...iconProps} />
	})}
</div>
