---
import LinkIcon from '../common/LinkIcon.astro'

interface Props {
	
}


const link_props = [
	{
		href: "/",
		displayName: "home"
	},
	{
		href: "/blog",
		displayName: "blog"
	},
	{
		href: "/about",
		displayName: "about"
	},
]
---
<div class="flex flex-col gap-2">
	{link_props.map(iconProps => {
	return <LinkIcon {...iconProps} />
	})}
</div>
